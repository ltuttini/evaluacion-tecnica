{
  "Kafka": {
    "BootstrapServers": "<local ip>:9092",
    "GroupId": "TransactionConsumer",
    "Topic": "TransactionTopic"
  },
  "Transaction": {
    "Url": "http://<local ip>:8080/Transaction/changestate"
  },
  "AntiFraud": {
    "AmountLimit": "2000",
    "AccumulatedPerDay": "20000"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
